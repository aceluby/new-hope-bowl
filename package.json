{
  "name": "new-hope-bowl",
  "version": "0.0.0",
  "license": "MIT",
  "main": "server/app.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json & npm run _server:run",
    "build": "ng build",
    "prod:build": "ng build --prod --aot --output-path=dist/client && cross-env NODE_ENV=production npm run _server:build",
    "_server:build": "tsc -p ./server",
    "export_variables": "export NODE_PATH=/app/node_modules",
    "prod:server": "NODE_ENV=production node dist/server/app.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/app.js\" ",
    "startHeroku": "export NODE_PATH=/app/node_modules && NODE_ENV=development && npm run build && npm run start_prod && npm run _server:runHeroku",
    "_server:runHeroku": "tsc -p ./server && node dist/server/app.js",
    "postinstall": "ng build --aot -prod",
    "deploy": "git push heroku master"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.0",
    "@angular/cli": "1.1.1",
    "@angular/common": "^4.2.0",
    "@angular/compiler": "^4.2.0",
    "@angular/compiler-cli": "^4.2.0",
    "@angular/core": "^4.2.0",
    "@angular/forms": "^4.2.0",
    "@angular/http": "^4.2.0",
    "@angular/language-service": "^4.2.0",
    "@angular/material": "^2.0.0-beta.6",
    "@angular/platform-browser": "^4.2.0",
    "@angular/platform-browser-dynamic": "^4.2.0",
    "@angular/router": "^4.2.0",
    "@types/body-parser": "^1.16.4",
    "@types/compression": "0.0.33",
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.66",
    "@types/node": "^6.0.85",
    "@types/pg": "^6.1.42",
    "@types/serve-favicon": "^2.2.28",
    "agentkeepalive": "^3.3.0",
    "angular2-text-mask": "^8.0.2",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "concurrently": "^3.5.0",
    "core-js": "^2.4.1",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "dateformat": "^2.0.0",
    "express": "^4.15.3",
    "lodash": "^4.17.4",
    "mdi": "^1.9.33",
    "moment": "^2.18.1",
    "node-gyp": "^3.6.2",
    "nodemon": "^1.11.0",
    "pg": "^7.0.1",
    "rxjs": "^5.1.0",
    "serve-favicon": "^2.4.3",
    "ts-node": "~3.0.4",
    "typescript": "^2.3.4",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.45",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "tslint": "~5.3.2"
  }
}
